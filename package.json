{
	"name": "svelte-golden-layout",
	"version": "0.1.1",
	"description": "GoldenLayout 2.x Wrapper for Svelte",
	"keywords": [
		"svelte",
		"golden-layout"
	],
	"author": "Clemens Koza",
	"repository": "github:SillyFreak/svelte-golden-layout",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"package": "vite package",
		"preview": "vite preview",
		"deploy": "npx gh-pages -d build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"peerDependencies": {
		"golden-layout": "^2.3.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "1.15.9",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.42.6",
		"svelte-check": "^3.2.0",
		"svelte-preprocess": "^5.0.3",
		"svelte-writable-derived": "^3.0.1",
		"svelte2tsx": "^0.6.11",
		"tslib": "^2.3.1",
		"typescript": "^5.0.4",
		"vite": "^4.3.3",
		"vite-plugin-svelte": "^3.0.1"
	},
	"dependencies": {
		"golden-layout": "^2.6.0"
	}
}
